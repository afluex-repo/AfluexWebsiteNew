
@{
    Layout = null;
}

<!DOCTYPE html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/afluexit/images/favicon.png" rel="shortcut icon">
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
    <title>Thank you </title>
    <style>
        .logo img{
            padding: 30px;
        }
        .thanku-footer.text-white.text-center {
    position: absolute;
   left: 0;
   right: 0;
    bottom: 0;
}
        .thanku-footer.text-white.text-center p {
    margin: 5px;
    font-size: 14px;
}
        p.msg-content {
    font-size: 18px;
    font-style: italic;
}
        .thankyou-box{
            border: 2px solid;
    border-radius: 10px;
    margin: 25px;
    padding: 60px;
    background-color: #ffffff1a;
        }
        .client-detail p {
    margin: 5px;
}
        span.p {
    font-weight: 500;
}
    </style>

    <link href="../../afluexad/css/bootstrap.css" rel="stylesheet" />
</head>
<html>
<body>
    @if (Session["Type"] == null)
    {
        <div class="thankyou-page" style="background-image: url('../afluexit/images/slider/tbg.jpg'); background-size: cover; background-repeat: no-repeat; height: 100vh; ">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-xl-12 col-xs-12 col-sm-12">
                            <div class="main-header">
                                <div class="logo text-center">
                                    <a href="/WebSoftware/index">
                                        <img src="~/afluexit/images/Afluex-logo/footer_logo.png" class="img-responsive" alt="logo" />
                                    </a>
                                </div>
                                
                                <div class="d-flex justify-content-center text-white mt-5">
                                    <div class="client-detail ">
                                        <p><span class="p">Demo URL : </span><span><em>gbfjhbkf@gmail.com</em></span></p>
                                        <div class="d-flex">
                                            <p><span class="p">Login Id : </span><span><em>Sachin Tiwari</em></span></p>
                                            <p><span class="p">Password : </span><span><em>123456</em></span></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="thanku-section text-white text-center d-flex justify-content-center">
                                    <div class="thankyou-box">
                                        <h1><em>Thank You!</em></h1>
                                        <p class="msg-content">For Enquiry. Our Team Will Contact You Soon !</p>
                                    </div>
                                </div>
                                <div class="thanku-footer text-white text-center">
                                    <p>Afluex Multiservices LLP</p>
                                    <p>'Our Expertise in : Advertising / IT Services'</p>
                                    <p>Phone No : +91 731-0000-413 / 414 / 05223550791 Email : supportnow@afluex.com</p>
                                    <p>Website : https://www.afluex.com/</p>

                                    <h6 class="mt-5">© Copyright 2017 All Rights Reserved</h6>
                                </div>
                            </div>
                        
                    </div>
                </div>
            </div>
        </div>
    }
    else if (Session["Type"] == "contact")
    {
        <div class="thankyou-page" style="background-image: url('../../afluexad/images/thank-you-page1.jpg');background-size: cover;background-repeat: no-repeat;">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-xl-12 col-xs-12 col-sm-12">
                        <div class="main-page1">
                            <div class="main-header">
                                <div class="logo text-center">
                                    <img src="../../afluexad/images/logo-load.png" class="img-responsive" alt="logo" />
                                </div>
                                <div class="thanku-section text-center">
                                    <h5>Thank you! <br>For contacting us. Our team will contact you soon !</h5>
                                </div>
                                <div class="thanku-footer text-center">
                                    <h5>Afluex Multiservices LLP</h5>
                                    <h5>'Our Expertise in : Advertising / IT Services'</h5>
                                    <h5>Phone No : +91 731-0000-413 / 414/05223550791 Email : supportnow@afluex.com</h5>
                                    <h5>Website : https://www.afluex.com/</h5>
                                </div>
                                <h5>© Copyright 2017 All Rights Reserved</h5>
                                <div class="thanku">
                                    <img src="../../afluexad/images/thank-you-page.png" class="img-responsive" alt="thank you">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    else
    {
        <div class="thankyou-page" style="background-image: url('../../afluexad/images/thank-you-page1.jpg');background-size: cover;background-repeat: no-repeat;">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-xl-12 col-xs-12 col-sm-12">
                        <div class="main-page2">
                            <div class="main-header">
                                <div class="logo text-center">
                                    <img src="~/afluexit/images/loader.png" class="img-responsive" alt="logo" />
                                </div>
                                <div class="thanku-section">


                                    @if (Session["Type"].ToString() == "mlm-software" || Session["Type"].ToString() == "real-estate-software" || Session["Type"].ToString() == "nidhi-software" || Session["Type"].ToString() == "helping-software")
                                    {
                                        <h5 class="text-center">Thank you! For Requesting Demo!</h5>
                                        <h4>Dear @Session["Name"]</h4>
                                        <h4>Below is your login Creditentials :-</h4>
                                        <a href='http://@Session["URL"]' target="_blank" class="btn btn-primary">Click Here For Open Demo</a>
                                        <div class="clearfix"></div>
                                        <br />
                                        <div class="loginfor">
                                            <h4><strong>Admin Panel</strong></h4>
                                            <br />
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Login Id</label>
                                                        <input readonly="readonly" class="form-control" value="@Session["AdminID"]" id="exampleInputEmail1" placeholder="Enter Login Id">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Password</label>
                                                        <input readonly="readonly" class="form-control" id="pass" value="@Session["AdminPass"]" placeholder="Enter Password">
                                                    </div>
                                                </div>

                                            </div>
                                            <h4><strong>Associate Panel</strong></h4>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Login Id</label>
                                                        <input readonly="readonly" class="form-control" value="@Session["UserName"]" id="exampleInputEmail1" placeholder="Enter Login Id">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Password</label>
                                                        <input readonly="readonly" class="form-control" value="@Session["Password"]" id="exampleInputEmail1" placeholder="Enter Login Id">
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    }
                                    else if (Session["Type"].ToString() == "ecommerce-software" || Session["Type"].ToString() == "school-management-software" ||
                                                   Session["Type"].ToString() == "travel-portal-software" ||
                                                   Session["Type"].ToString() == "flight-booking-software" || Session["Type"].ToString() == "hospital-management-software" ||
                                                   Session["Type"].ToString() == "daily-expenses-software" ||
                                                   Session["Type"].ToString() == "hrms-software" || Session["Type"].ToString() == "mobile-recharge-software" || Session["Type"].ToString() == "news-portal-software")
                                    {
                                        <h5 class="text-center">Thank you! For Requesting Demo!</h5>
                                        <h4>Dear @Session["Name"]</h4>
                                        <h4>Below is your login Creditentials :-</h4>
                                        <a href='http://@Session["URL"]' target="_blank" class="btn btn-primary">Click Here For Open Demo</a>
                                        <div class="clearfix"></div>
                                        <br />
                                        <div class="loginfor">
                                            <h4><strong>Admin Panel</strong></h4>
                                            <br />
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Login Id</label>
                                                        <input readonly="readonly" class="form-control" value="@Session["AdminID"]" id="exampleInputEmail1" placeholder="Enter Login Id">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Password</label>
                                                        <input readonly="readonly" class="form-control" id="pass" value="@Session["AdminPass"]" placeholder="Enter Password">
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <h5 class="text-center">Dear @Session["Name"] For Enquiry.Our team will contact you soon !</h5>
                                    }
                                </div>
                                <div class="thanku-footer text-center ">
                                    <h5>Afluex Multiservices LLP</h5>
                                    <h5>'Our Expertise in : Advertising / IT Services'</h5>
                                    <h5>Phone No : +91 731-0000-413 / 414/05223550791 Email : supportnow@afluex.com</h5>
                                    <h5>Website : https://www.afluex.com/</h5>
                                </div>
                                <h5>© Copyright 2017 All Rights Reserved</h5>
                                <div class="thanku ">
                                    <img src="../../afluexad/images/thank-you-page.png " class="img-responsive " alt="thank you ">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="../../afluexad/js/bootstrap.min.js"></script>
</body>

</html>
