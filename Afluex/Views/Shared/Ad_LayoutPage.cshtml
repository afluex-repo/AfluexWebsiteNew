        function SaveInquiryDetails() {
            $(".errortext").removeClass("errortext");
            var InquiryType = 'AD';
            if ($("#name").val() == '') {
                $("#name").addClass('errortext');
                $('#name').focus();
                return false;
            }
            if ($("#Email").val() == '') {
                $("#Email").addClass('errortext');
                $('#Email').focus();
                return false;
            }
            if ($("#mobile").val() == '') {
                $("#mobile").addClass('errortext');
                $('#mobile').focus();
                return false;
            }
            if ($("#ProjectType").val() == '') {
                $("#ProjectType").addClass('errortext');
                $('#ProjectType').focus();
                return false;
            }
            if ($("#YourBudget").val() == '') {
                $("#YourBudget").addClass('errortext');
                $('#YourBudget').focus();
                return false;
            }
            if ($("#Message").val() == '') {
                $("#Message").addClass('errortext');
                $('#Message').focus();
                return false;
            }
            $.ajax({
                url: "/Home/SaveInquiryDetails",
                Type: "Get",
                DataType: "JSON",
                data: { Name: $("#name").val(), Email: $("#Email").val(), Mobile: $("#mobile").val(), ProjectType: $("#ProjectType").val(), YourBudget: $("#YourBudget").val(), Message: $("#Message").val(), InquiryType: InquiryType },
                success: function (data) {
                    $("#name").val('');
                    $("#Email").val('');
                    $("#mobile").val('');
                    $("#projecttype").val('');
                    $("#yourbudget").val('');
                    $("#message").val('');
                    window.location.href = '@Url.Action("thankyou", "advertisement")';
                }
            });
        }

        function ValidateEmailIDCheck(event) {
            debugger
            var panVal = $('#Email').val();
            var regpan = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;

            if (regpan.test(panVal)) {
                $("#Email").removeClass('errortext');
            } else {
                alert("Invalid Email ID");
                $("#Email").val('');
                $("#Email").addClass('errortext');
            }
        }
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (charCode != 46 && charCode > 31
              && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }
